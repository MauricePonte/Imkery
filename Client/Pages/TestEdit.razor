@page "/test"
@page "/test/{itemId:guid}"
@using Imkery.Client.Components.ObjectFormComponent
@using Imkery.Entities

<MudPaper>
    <ObjectForm TItem="Test" ItemId="@ItemId" ReturnUrl="@(item=> "tests")">
        <FormContent>
            <MudTextField Label="Name" @bind-Value="@context.Name" Required="true"/>
            <MudNumericField Label="Age" @bind-Value="@context.Age" Required="true"/>
        </FormContent>
    </ObjectForm>
</MudPaper>


@code
{
    [Parameter]
    public Guid ItemId { get; set; }
 }